# Inside predictive_optimal_combinations_advanced()
window_size = window_max - window_min

# Generate time points based on window size
if window_min == 0 and window_max == 60:
    # 0-60: Combine 3 & 5 minute steps
    times_3 = list(range(0, 61, 3))
    times_5 = list(range(0, 61, 5))
    times_list = sorted(list(set(times_3 + times_5)))
else:
    # Other windows: 5-minute steps only
    times_list = list(range(window_min, window_max + 1, 5))

# Inside search_sequence()
# Ensure sequential ordering (start to end)
if any(seq[i] >= seq[i+1] for i in range(len(seq)-1)):
    return  # Skip non-sequential combinations
