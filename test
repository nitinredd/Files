Local Host
docker pull ghcr.io/nlmatics/nlm-ingestor:latest
docker run -p 5010:5001 ghcr.io/nlmatics/nlm-ingestor:latest
url: http://localhost:5010/api/parseDocument?renderFormat=all

GCP Host
docker pull ghcr.io/nlmatics/nlm-ingestor:latest
docker tag ghcr.io/nlmatics/nlm-ingestor:latest gcr.io/[YOUR-PROJECT-ID]/nlm-ingestor:latest
docker push gcr.io/[YOUR-PROJECT-ID]/nlm-ingestor:latest
gcloud config set project [id]
gcloud container clusters create llmsherpa-cluster --num-nodes=3 --zone=us-central1-a
gcloud container clusters get-credentials llmsherpa-cluster --zone=us-central1-a
llmsherpa-deployment.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: llmsherpa-deployment
spec:
  replicas: 3
  selector:
    matchLabels:
      app: llmsherpa
  template:
    metadata:
      labels:
        app: llmsherpa
    spec:
      containers:
      - name: llmsherpa
        image: gcr.io/[YOUR-PROJECT-ID]/nlm-ingestor:latest
        ports:
        - containerPort: 5001
---
apiVersion: v1
kind: Service
metadata:
  name: llmsherpa-service
spec:
  type: LoadBalancer
  selector:
    app: llmsherpa
  ports:
  - port: 80
    targetPort: 5001
kubectl apply -f llmsherpa-deployment.yaml
kubectl get services
URL: External IP
